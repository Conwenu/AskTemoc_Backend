# Environment Configuration Template

# Database Configuration
# SQLite (default):
DATABASE_URL=sqlite:///./asktemoc.db

# PostgreSQL (optional):
# DATABASE_URL=postgresql://user:password@localhost/asktemoc

# Enable database query logging
DB_ECHO=false

